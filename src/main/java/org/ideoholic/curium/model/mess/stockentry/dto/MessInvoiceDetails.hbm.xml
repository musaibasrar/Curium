<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.mess.stockentry.dto.MessInvoiceDetails" table="mess_invoicedetails" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <!-- <generator class="identity" /> -->
            <generator class="native" />
        </id>
        <property name="externalid" type="string">
            <column name="externalid" />
        </property>
        <property name="supplierreferenceno" type="string">
            <column name="supplierreferenceno" />
        </property>
        <property name="entrydate" type="date">
            <column name="entrydate" length="10" />
        </property>
        <property name="invoicedate" type="date">
            <column name="invoicedate" length="10" />
        </property>
        <property name="invoicetotal" type="java.lang.Float">
            <column name="invoicetotal" />
        </property>
        <property name="branchid" type="java.lang.Integer">
            <column name="branchid" />
        </property>
          <property name="suppliersid" type="java.lang.Integer">
            <column name="suppliersid" />
        </property>
         <property name="status" type="string">
            <column name="status" />
        </property>
            <property name="voucherid" type="java.lang.Integer">
            <column name="voucherid" />
        </property>
        <property name="userid" type="int">
 <column name="userid" />
</property>
    </class>
</hibernate-mapping>
